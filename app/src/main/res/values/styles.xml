<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <!-- extend one of the Theme.AppCompat themes -->
    <style name="AppThemeSec" parent="Theme.AppCompat.Light">
        <!-- customize the color palette -->
        <item name="colorPrimary">#329da3</item>
        <item name="colorPrimaryDark">#266779</item>
        <item name="colorAccent">#eee</item>
        <item name="android:textColorPrimary">#fff</item>
    </style>

    <style name="AppThemeFour" parent="android:Theme.Holo.Light.NoActionBar.TranslucentDecor">
        <!-- Status Bar -->
        <item name="android:windowTranslucentStatus">true</item>
        <!-- Navigation Bar -->
        <item name="android:windowTranslucentNavigation">true</item>
    </style>
</resources>
